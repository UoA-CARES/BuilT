    "version": "0.2.0",
    "configurations": [
        {
            "name": "Train_sentiment_classification",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "args": [
                "train",
                "with",
                "${workspaceFolder}/tweet/config/tweet_sentiment_classification.yaml",
                "-f"
            ],
            "env": {
                "TOKENIZERS_PARALLELISM": "True"
            }
        },
        {
            "name": "Train_index_extraction",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "args": [
                "train",
                "with",
                "${workspaceFolder}/tweet/config/tweet_index_extraction.yaml",
                "-f",
                "use_date=True"
            ],
            "env": {
                "TOKENIZERS_PARALLELISM": "True"
            }
        },
        {
            "name": "Train_index_extraction_coverage",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "args": [
                "train",
                "with",
                "${workspaceFolder}/tweet/config/tweet_index_extraction_coverage.yaml",
                "-f",
                "use_date=True"
            ],
            "env": {
                "TOKENIZERS_PARALLELISM": "True"
            }
        },
        {
            "name": "Ensemble",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "args": [
                "ensemble",
                "with",
                "${workspaceFolder}/tweet/config/tweet_sentiment_classification.yaml",
                "-f"
            ],
            "env": {
                "TOKENIZERS_PARALLELISM": "True"
            }
        }
    ]
}